<div class="full-table">
  <table>
    <tr>
      <!-- select first product and use its keys as the column names -->
      {{#each-in productData.[1] as |key|}}
      <th>{{key}}</th>
      {{/each-in}}
    </tr>
    {{#each productData as |data|}}
      <tr>
        {{#each-in data as |key value|}}
          <td>{{value}}</td>
        {{/each-in}}
      </tr>
    {{/each}}
  </table>
</div>
{{outlet}}
